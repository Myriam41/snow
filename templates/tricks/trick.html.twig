{% extends 'base.html.twig' %}

{% block title %}Tricks {{ controller_name }}!{% endblock %}

{% block body %}
<h1>{{ Trick.Type }}  {{ Trick.Name }}</h1>

<article>
    <div class="metadata"> 
        {{ Trick.Switch }}
    </div>
    <div class="content">
        {{ Trick.Description | raw}}
    </div>
</article>

{% endblock %}
